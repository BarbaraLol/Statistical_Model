result_table <- data.frame(
Model = c("Linear model with only GDP", "Multiple Regression"),
Coefficient = c(single_coeff, multiple_coeff),
Std_Error = c(single_se, multiple_se),
Adj_R_squared = c(single_model_r2, multiple_model_r2)
)
